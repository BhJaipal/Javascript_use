{
    "[typescriptreact]": {
        "editor.autoIndent": "advanced",
        "anycode.language.features": {
            "completions": true,
            "definitions": true,
            "references": true,
            "highlights": true,
            "outline": true,
            "workspaceSymbols": true,
            "folding": false,
            "diagnostics": false,
        },
        "debug.saveBeforeStart": "allEditorsInActiveGroup",
